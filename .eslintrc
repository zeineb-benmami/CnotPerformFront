{
  "rules": {
    "no-undef": "off"
  },
  "root": true,
  "env": { "browser": true, "es2020": true },
  "node": true,
  "extends": ["plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  }
}
